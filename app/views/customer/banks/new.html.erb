<% @title = '口座の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @bank_form, as: 'form', url: :customer_bank do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '登録', class: "btn btn-default", data: {disable_with: '登録中・・・' } %>
      <%= link_to 'キャンセル', :customer_root %>
    </div>
  <% end %>
</div>

