<table class="attributes">
  <tr><th colspan= "3">※<%= @sf_name %>さんの列のボーナス成績(<%= @q %>クオーター）</th></tr>
  <tr><th>紹介者氏名</th><td><%= @sf_name %></td></tr>
  <tr><th>ニックネーム</th><td><%= @sn_name %></td></tr>
  <% if @rgpt == "退会" %>
    <tr><th>在籍状況</th><td><strong><%= @rgpt %></strong>　しています。</td></tr>
  <% else %>
    <tr><th>在籍状況</th><td><%= @rgpt %>　しています。</td></tr>
  <% end %>
  <tr><th><%= @sf_name %>さん個人(pt)</th><td><%= number_with_delimiter(@s_point) %>pt</td></tr>
  <tr><th><%= @sf_name %>さん列の(pt)</th><td><%= @bbb %>pt</td></tr>
  <tr><th>列のタイトル</th><td><%= @b_title %></td></tr>
</table>