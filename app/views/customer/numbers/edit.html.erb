<% @title = 'バックナンバー検索' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @number_form, as: 'form', url: [ :customer, @number_form.number ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '検索', class: "btn btn-default", data: {disable_with: '検索中・・・' } %>
      <%= link_to 'キャンセル', :customer_root %>
    </div>
  <% end %>
</div>
