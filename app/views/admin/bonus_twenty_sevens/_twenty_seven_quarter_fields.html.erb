<% if @quarter1 == 4 %>
  <%= f.fields_for :twenty_six, f.object.twenty_six do |ff| %>
    <%= markup do |m|
      p = confirming ? ConfirminTwentySixFormPresenter.new(ff,self) :  TwentySixFormPresenter.new(ff, self)
      p.with_options(required: true) do |q|
        m << q.drop_down_list_block(:transfer_c_4, '４クオーターボーナス送金', TwentySix::TRANSFER)
      end
    end %>
  <% end %>
<% else%>
  <%= f.fields_for :twenty_seven, f.object.twenty_seven do |ff| %>
    <% if @quarter1 == 1 %>
      <%= markup do |m|
        p = confirming ? ConfirminTwentySevenFormPresenter.new(ff,self) :  TwentySevenFormPresenter.new(ff, self)
        p.with_options(required: true) do |q|
          m << q.drop_down_list_block(:transfer_c_1, '1クオーターボーナス送金', TwentySeven::TRANSFER)
        end
      end %>
    <% elsif @quarter1 == 2 %>
      <%= markup do |m|
        p = confirming ? ConfirminTwentySevenFormPresenter.new(ff,self) :  TwentySevenFormPresenter.new(ff, self)
        p.with_options(required: true) do |q|
          m << q.drop_down_list_block(:transfer_c_2, '２クオーターボーナス送金', TwentySeven::TRANSFER)
        end
      end %>
    <% else %>
      <%= markup do |m|
        p = confirming ? ConfirminTwentySevenFormPresenter.new(ff,self) :  TwentySevenFormPresenter.new(ff, self)
        p.with_options(required: true) do |q|
          m << q.drop_down_list_block(:transfer_c_3, '3クオーターボーナス送金', TwentySeven::TRANSFER)
        end
      end %>
    <% end %>
  <% end %>
<% end %>