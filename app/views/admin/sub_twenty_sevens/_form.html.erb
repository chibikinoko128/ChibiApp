<%= FormPresenter.new(f, self).notes %>
<% if @choice_title == "キャッシュバック" %>  
  <fieldset id="TwentySeven-fields">
  < legend><%= @acc_name %>（<%= @nickname %>）様の<%= @year %> 年　<%= @month %> 月のキャッシュバック送金履歴更新</legend>
  <%= render 'twenty_seven_fields', f: f, confirming: false %>
  </fieldset>
<% elsif @choice_title == "ボーナス" %>
  <fieldset id="TwentySeven-fields">
  <legend><%= @acc_name %>（<%= @nickname %>）様の<%= @year %> 年　<%= @quarter %> クオーター送金履歴更新</legend>
  <%= render 'twenty_seven_fields', f: f, confirming: false %>
  </fieldset>
<% else %>
  <% if @choice_title == "特別会員ボーナス" %>
    <fieldset id="TwentySeven-fields">
    <legend><%= @acc_name %>（<%= @nickname %>）様の<%= @year %> 年　<%= @month %> 月の特別会員ボーナス送金履歴更新</legend>
    <%= render 'twenty_seven_fields', f: f, confirming: false %>
    </fieldset>
  <% end %>
<% end %>  