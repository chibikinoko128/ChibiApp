<% @title = '年月・クオータ別　キャッシュバック・ボーナス獲得者検索' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @pay_form, as: 'form', url: [ :admin, @pay_form.pay ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '検索', class: "btn btn-default", data: {disable_with: '検索中・・・' } %>
      <%= link_to 'キャンセル', :admin_root %>
    </div>
  <% end %>
</div>
