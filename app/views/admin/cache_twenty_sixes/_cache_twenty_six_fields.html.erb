      <%= markup do |m|
        p = confirming ? ConfirminTwentyFiveFormPresenter.new(ff, self) : TwentyFiveFormPresenter.new(ff, self)
        p.with_options(required: true) do |q|
          m << q.drop_down_list_block(:transfer_m_11, '11月分キャッシュバック送金', TwentyFive::TRANSFER)
        end
      end %>
    <% else %>
