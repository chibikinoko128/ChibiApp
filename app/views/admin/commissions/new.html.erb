<% @title = '振込み手数料の新規登録' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @commission_form, as: 'form', url: :admin_commissions do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '登録', class: "btn btn-default", data: {disable_with: '登録中・・・' } %>
      <%= link_to 'キャンセル', :admin_root %>
    </div>
  <% end %>
</div>

<div class="table-wrapper">
  <div class="links">
    <%= link_to 'トップ画面へ戻る', :admin_root %>
  </div>
</div>