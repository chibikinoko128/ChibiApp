<% @title = '年度別成績検索' %>
<h1><%= @title %></h1>

<div id="generic-form">
  <%= form_for @staff_year_form, as: 'form', url: [ :staff, @staff_year_form.staff_year ] do |f| %>
    <%= render 'form', f: f %>
    <div class="buttons">
      <%= f.submit '検索', class: "btn btn-default", data: {disable_with: '検索中・・・' } %>
      <%= link_to 'キャンセル', :staff_root %>
    </div>
  <% end %>
</div>
